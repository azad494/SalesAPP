<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.SwipeRefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/swipe_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <android.support.v4.widget.NestedScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fillViewport="true">

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/Sales_return"
                android:textAlignment="center"
                android:textColor="@color/basic_title"
                android:textSize="24sp"
                android:textStyle="bold" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/Date1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/basic_height"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:layout_marginBottom="@dimen/basic_MarginBottom"
                    android:background="@drawable/textbox_design"
                    android:clickable="true"
                    android:enabled="false"
                    android:focusable="false"
                    android:gravity="center"
                    android:hint="Date" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:layout_marginBottom="@dimen/basic_MarginBottom"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="4"
                        android:text="Customer"
                        android:gravity=""
                        android:textSize="@dimen/basic_font"
                        android:textStyle="normal|bold" />

                    <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                        android:id="@+id/spCustomerName"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/basic_height"
                        android:background="@drawable/spinner_bg"
                        android:layout_weight="2" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:layout_marginBottom="@dimen/basic_MarginBottom"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="4"
                        android:text="Sales Invoice"
                        android:gravity=""
                        android:textSize="@dimen/basic_font"
                        android:textStyle="normal|bold" />

                    <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                        android:id="@+id/spSaleInvoice"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/basic_height"
                        android:background="@drawable/spinner_bg"
                        android:layout_weight="2" />
                </LinearLayout>

                <EditText
                    android:visibility="gone"
                    android:id="@+id/salesReturnDetails"
                    android:enabled="false"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textMultiLine"
                    android:maxLines="3"
                    android:paddingLeft="10dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:layout_marginBottom="@dimen/basic_MarginBottom"
                    android:background="@drawable/desabale_textbox_design"
                    android:gravity="center"
                    android:hint="Sales Details"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:layout_marginBottom="@dimen/basic_MarginBottom"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="4"
                        android:text="Product"
                        android:gravity=""
                        android:textSize="@dimen/basic_font"
                        android:textStyle="normal|bold" />

                    <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                        android:id="@+id/spProduct"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/basic_height"
                        android:background="@drawable/spinner_bg"
                        android:layout_weight="2" />
                    <Spinner
                        android:visibility="gone"
                        android:id="@+id/spProduct2"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/basic_height"
                        android:background="@drawable/spinner_bg"
                        android:layout_weight="2">

                    </Spinner>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:layout_marginBottom="@dimen/basic_MarginBottom"
                    android:orientation="horizontal"
                    android:weightSum="6">

                    <TextView
                        android:layout_width="@dimen/label_width"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="2"
                        android:text="Return MRP"
                        android:textSize="@dimen/basic_font"
                        android:textStyle="normal|bold"/>
                    <EditText
                        android:id="@+id/txtMRP"
                        android:layout_width="@dimen/label_width"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="1.9"
                        android:textSize="@dimen/basic_font"
                        android:textStyle="normal|bold"
                        android:background="@drawable/textbox_design"
                        android:gravity="center"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.2"/>

                    <EditText
                        android:id="@+id/txtHmrp"
                        android:layout_width="@dimen/label_width"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="1.9"
                        android:background="@drawable/desabale_textbox_design"
                        android:enabled="false"
                        android:gravity="center"
                        android:textSize="@dimen/basic_font"
                        android:textStyle="normal|bold" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:layout_marginBottom="@dimen/basic_MarginBottom"
                    android:orientation="horizontal"
                    android:weightSum="6">

                    <TextView
                        android:layout_width="@dimen/label_width"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="2"
                        android:text="Return Qty
"
                        android:textSize="@dimen/basic_font"
                        android:textStyle="normal|bold"/>
                    <EditText
                        android:id="@+id/txtQut"
                        android:layout_width="@dimen/label_width"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="1.9"
                        android:textSize="@dimen/basic_font"
                        android:textStyle="normal|bold"
                        android:background="@drawable/textbox_design"
                        android:gravity="center"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.2"/>
                    <EditText
                        android:id="@+id/txtHQty"
                        android:enabled="false"
                        android:layout_width="@dimen/label_width"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="1.9"
                        android:textSize="@dimen/basic_font"
                        android:textStyle="normal|bold"
                        android:background="@drawable/desabale_textbox_design"
                        android:gravity="center"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:layout_marginBottom="@dimen/basic_MarginBottom"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="4"
                        android:text="Warehouse"
                        android:gravity=""
                        android:textSize="@dimen/basic_font"
                        android:textStyle="normal|bold" />
                    <Spinner
                        android:id="@+id/spWareHouse"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/basic_height"
                        android:background="@drawable/spinner_bg"
                        android:layout_weight="2">

                    </Spinner>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:layout_marginBottom="@dimen/basic_MarginBottom"
                    android:weightSum="2.5">

                    <Button
                        android:id="@+id/btnReset"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="1"
                        android:textStyle="bold"
                        android:textColor="@color/basic_title"
                        android:background="@drawable/textbox_design"
                        android:text="Reset"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"/>
                    <Button
                        android:id="@+id/btnAdd"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="1"
                        android:textStyle="bold"
                        android:background="@drawable/textbox_design"
                        android:textColor="@color/basic_title"
                        android:text="Add"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:layout_marginBottom="@dimen/basic_MarginBottom"
                    android:background="@color/basic_title"
                    android:weightSum="5">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:text="@string/Product"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textStyle="bold" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textStyle="bold"
                        android:textColor="@color/white"
                        android:text="Qty"
                        android:textAlignment="center"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textStyle="bold"
                        android:textColor="@color/white"
                        android:text="RRPU"
                        android:textAlignment="center"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textStyle="bold"
                        android:textColor="@color/white"
                        android:text="@string/Total"
                        android:textAlignment="center"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:orientation="vertical"
                    android:weightSum="3">
                    <ListView
                        android:layout_weight="1"
                        android:id="@android:id/list"
                        android:layout_width="match_parent"
                        android:layout_height="200dp"
                        android:alwaysDrawnWithCache="true"
                        android:horizontalSpacing="3dp"
                        android:scrollingCache="true"
                        android:smoothScrollbar="true"
                        android:stretchMode="columnWidth"
                        android:divider="@color/basic_title"
                        android:dividerHeight="1dp"
                        android:verticalSpacing="3dp">
                    </ListView>

                </LinearLayout>
                <LinearLayout
                    android:visibility="visible"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:layout_marginBottom="@dimen/basic_MarginBottom"
                    android:orientation="horizontal"
                    android:weightSum="6">

                    <TextView
                        android:layout_width="@dimen/label_width"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="2"
                        android:text="Total"
                        android:textSize="@dimen/basic_font"
                        android:textStyle="normal|bold"/>
                    <EditText
                        android:id="@+id/txtHTotal"
                        android:enabled="false"
                        android:layout_width="@dimen/label_width"
                        android:layout_height="@dimen/basic_height"
                        android:layout_weight="4"
                        android:textSize="@dimen/basic_font"
                        android:textStyle="normal|bold"
                        android:background="@drawable/desabale_textbox_design"
                        android:gravity="center"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginLeft="@dimen/spinner_margin_l"
                    android:layout_marginRight="@dimen/spinner_margin_l"
                    android:layout_marginBottom="@dimen/basic_MarginBottom"
                    android:weightSum="2.1">
                    <Button
                        android:id="@+id/btnCancel"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textStyle="bold"
                        android:background="@drawable/cancel_button"
                        android:textColor="@color/white"
                        android:text="@string/cancel"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.1"/>

                    <Button
                        android:id="@+id/btnSave"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textStyle="bold"
                        android:textColor="@color/white"
                        android:background="@drawable/single_btn"
                        android:text="@string/save"/>

                </LinearLayout>
            </LinearLayout>

        </android.support.v7.widget.LinearLayoutCompat>

    </android.support.v4.widget.NestedScrollView>
</android.support.v4.widget.SwipeRefreshLayout>



